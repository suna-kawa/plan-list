<% if @plan.errors.any? %>
  <div id="error_explanation">
  <div class="alert alert-danger">
    <p><%= pluralize(@plan.errors.count, "error") %>が原因で保存出来ませんでした</p>
    <ul>
    <% @plan.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
  </div>
<% end %>  
